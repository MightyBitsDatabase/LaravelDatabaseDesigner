{
  "name": "MightyBits",
  "version": "0.0.1",
  "description": "MightyBits Open Data Designer",
  "main": "main.js",
  "scripts": {
    "start": "electron main.js",
    "buildosx": "electron-packager . --platform=darwin --arch=x64 --overwrite --asar=true",
    "buildwin": "export PATH=$PATH:/Users/xcorex/Library/PlayOnMac/wine/darwin-x86/1.9.5-staging/bin/; electron-packager . --platform=win32 --arch=ia32 --overwrite --asar=true",
    "buildlinux": "electron-packager . cocoblock --platform=linux --arch=ia32 --asar=true --overwrite",
    "buildall": "npm run-script buildosx ; npm run-script buildwin ;npm run-script buildlinux "    
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BioDesignRealWorld/LaravelDatabaseDesigner.git"
  },
  "keywords": [
    "Laravel",
